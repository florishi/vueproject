<form>
  <div class="form-group">
    <label for="mood">Mood</label>
    <input type="text" class="form-control" id="mood" placeholder="Mood">
  </div>
  <div class="form-group">
    <label for="water">Water</label>
    <input type="number" class="form-control" id="water" placeholder="Water">
  </div>
  <div class="form-group">
    <label for="steps">steps</label>
    <input type="number" class="form-control" id="steps" placeholder="steps">
  </div>
  <div class="form-group">
    <label for="sleep">sleep</label>
    <input type="number" class="form-control" id="sleep" placeholder="sleep">
  </div>
  <div class="form-group">
    <label for="exercise">excercise</label>
    <input type="number" class="form-control" id="exercise" placeholder="excercise">
  </div>
