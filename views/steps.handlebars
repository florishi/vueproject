<h1>Steps</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Date Logged</th>
      <th scope="col">Your Steps </th>
    </tr>
  </thead>
  <tbody>
    {{#each getData }}
    <tr>
      <th scope="row">{{inputDate}}</th>
      <td>{{steps}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>