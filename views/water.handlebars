<h1>Water</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Date Logged</th>
      <th scope="col">Your water intake (litres)</th>
    </tr>
  </thead>
  <tbody>
    {{#each getData }}
    <tr>
      <th scope="row">{{inputDate}}</th>
      <td>{{waterUnits}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>